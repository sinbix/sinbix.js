{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@sinbix/docs-ngx-frontend/feat-main": [
        "libs/apps/docs/ngx-frontend/feat-main/src/index.ts"
      ],
      "@sinbix/docs-ngx-frontend/feat-home": [
        "libs/apps/docs/ngx-frontend/feat-home/src/index.ts"
      ],
      "@sinbix/docs-ngx-frontend/components-feat-main": [
        "libs/apps/docs/ngx-frontend/components/feat-main/src/index.ts"
      ],
      "@sinbix/docs-ngx-frontend/feat-shell": [
        "libs/apps/docs/ngx-frontend/feat-shell/src/index.ts"
      ],
      "@sinbix/common": ["libs/packages/common/src/index.ts"],
      "@sinbix/common/*": ["libs/packages/common/src/lib/*"],
      "@sinbix/angular": ["libs/packages/angular/src/index.ts"],
      "@sinbix/angular/*": ["libs/packages/angular/src/lib/*"],
      "@sinbix/angular-ui/*": ["libs/packages/angular-ui/src/lib/*"],
      "@sinbix/angular-ui": ["libs/packages/angular-ui/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
