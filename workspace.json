{
  "version": 1,
  "projects": {
    "schematics": {
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/schematics/tsconfig.lib.json",
              "libs/schematics/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/schematics/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "tsConfig": "libs/schematics/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
