{
  "version": 1,
  "projects": {
    "packages-ngx-utils": {
      "projectType": "library",
      "root": "libs/packages/ngx-utils",
      "sourceRoot": "libs/packages/ngx-utils/src",
      "prefix": "sinbix",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/packages/ngx-utils/tsconfig.lib.json",
            "project": "libs/packages/ngx-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/packages/ngx-utils/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/packages/ngx-utils/tsconfig.lib.json",
              "libs/packages/ngx-utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/packages/ngx-utils/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/packages/ngx-utils/jest.config.js",
            "tsConfig": "libs/packages/ngx-utils/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/packages/ngx-utils/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "demo-frontend": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo/frontend",
      "sourceRoot": "apps/demo/frontend/src",
      "prefix": "sinbix",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo/frontend",
            "index": "apps/demo/frontend/src/index.html",
            "main": "apps/demo/frontend/src/main.ts",
            "polyfills": "apps/demo/frontend/src/polyfills.ts",
            "tsConfig": "apps/demo/frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo/frontend/src/favicon.ico",
              "apps/demo/frontend/src/assets"
            ],
            "styles": ["apps/demo/frontend/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/frontend/src/environments/environment.ts",
                  "with": "apps/demo/frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-frontend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/frontend/tsconfig.app.json",
              "apps/demo/frontend/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo/frontend/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo/frontend/jest.config.js",
            "tsConfig": "apps/demo/frontend/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/demo/frontend/src/test-setup.ts"
          }
        }
      }
    },
    "demo/frontend-e2e": {
      "root": "apps/demo/frontend-e2e",
      "sourceRoot": "apps/demo/frontend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo/frontend-e2e/cypress.json",
            "tsConfig": "apps/demo/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo/frontend-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/demo/frontend-e2e/**/*"]
          }
        }
      }
    },
    "packages-ngx-ui": {
      "projectType": "library",
      "root": "libs/packages/ngx-ui",
      "sourceRoot": "libs/packages/ngx-ui/src",
      "prefix": "sinbix",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/packages/ngx-ui/tsconfig.lib.json",
            "project": "libs/packages/ngx-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/packages/ngx-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/packages/ngx-ui/tsconfig.lib.json",
              "libs/packages/ngx-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/packages/ngx-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/packages/ngx-ui/jest.config.js",
            "tsConfig": "libs/packages/ngx-ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/packages/ngx-ui/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "demo-frontend",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
